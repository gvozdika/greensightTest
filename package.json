{
    "name": "rwbp",
    "version": "3.3.0",
    "private": true,
    "repository": "https://gitlab.com/greensight/rwbp",
    "license": "MIT",
    "scripts": {
        "dev": "NODE_ENV=development ts-node -r dotenv/config src/server/dev.ts",
        "dev:measure": "MEASURE=true yarn dev",
        "eslint": "eslint . --ext .js,.ts,.tsx,.mdx --fix",
        "format": "prettier  . --write --ignore-path .gitignore",
        "lint-staged": "lint-staged",
        "prod": "run-s prod:build prod:serve",
        "prod:build": "run-p prod:tsc prod:build:server prod:build:client",
        "prod:build:client": "NODE_ENV=production webpack --env.isClient",
        "prod:build:server": "NODE_ENV=production webpack",
        "prod:measure": "MEASURE=true yarn prod",
        "prod:serve": "NODE_ENV=production node -r dotenv/config build/server/prod.js",
        "prod:tsc": "yarn pnpify tsc -p tsconfig.server.json",
        "storybook": "start-storybook -s ./public -p 8080 --ci",
        "storybook:build": "build-storybook -o docs -s ./public",
        "storybook:serve": "serve docs -l 5000",
        "tokens": "gds-tokens",
        "tsc": "yarn pnpify tsc",
        "pre-commit": "run-p lint-staged tsc",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:update": "jest -u",
        "test:watch": "jest --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn pre-commit"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,mdx}": [
            "eslint --fix",
            "yarn format",
            "jest --findRelatedTests"
        ]
    },
    "browserslist": [
        "chrome >= 67",
        "ios >= 11",
        "safari >= 12",
        "firefox >= 67",
        "edge >= 17",
        "opera >= 60"
    ],
    "eslintIgnore": [
        "node_modules",
        "public",
        "build",
        "docs",
        "!/.storybook"
    ],
