    "layout": {
        "cols": { "xxxl": 12, "md": 6, "xs": 4, "xxs": 2 },
        "container": { "xxxl": 1440, "xl": "none" },
        "marginLeft": { "xxxl": "auto", "xl": 0 },
        "marginRight": { "xxxl": "auto", "xl": 0 },
        "breakpoints": {
            "xxxl": 1920,
            "xxl": 1600,
            "xl": 1440,
            "lg": 1280,
            "md": 1024,
            "sm": 768,
            "xs": 568,
            "xxs": 360,
            "xxxs": 320
