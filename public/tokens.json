        "gap": { "xxxl": 24, "sm": 16 },
        "padding": { "xl": 60, "lg": 40, "md": 32, "xs": 24, "xxxs": 16 }
    },
    "shadows": {
        "small": "1px 1px 3px rgba(51, 51, 51, 0.03), 0 3px 9px rgba(51, 51, 51, 0.08)",
        "big": "4px 10px 35px rgba(51, 51, 51, 0.08)",
        "bigInversion": "0 -10px 35px rgba(51, 51, 51, 0.08)",
        "inner": "inset 3px 3px 10px rgba(51, 51, 51, 0.1), inset 0 0 3px rgba(51, 51, 51, 0.1)"
    },
